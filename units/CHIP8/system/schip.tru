Unit Schip;
var
	n: byte;
procedure HiRes() inline;
begin
	asm("HIGH");
end;	

procedure LowRes() inline;
begin
	asm("LOW");
end;	
procedure ScrollDown4(n : global byte);
begin
	if (n<16) then begin
		asm("
		LD V0, Schip_n
		ADD V0, #C0
		LD I, schip_scd_insertion+1
		LD [I], V0
		schip_scd_insertion:
		SCD 0
		");
	end;

end;
end.
