// memory inc test
procedure Test1();
begin
	poke(^$4000,0,10);
	inc(^$4000);	
	Vars::v1:=peek(^$4000,0);
	
	if (peek(^$4000,0)=11) then Common::PASS() else Common::FAIL();
end;

